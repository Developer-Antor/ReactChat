"login-container\" id=\"signin\">\n        <img\n          src=\"https://cdn0.iconfinder.com/data/icons/kameleon-free-pack-rounded/110/Chat-2-512.png\"\n          alt=\"\"\n        />\n        <input\n          placeholder=\"Email\"\n          value={email1}\n          onChange={(e) => {\n            setEmail1(e.target.value);\n          }}\n          type=\"text\"\n        />\n        <input\n          placeholder=\"Password\"\n          value={pass1}\n          onChange={(e) => {\n            setPass1(e.target.value);\n          }}\n          type=\"password\"\n        />\n        <Button\n          style={{ color: \"#fff\", margin: \"10px\", background: \"#67d3fb\" }}\n          className=\"login-button\"\n          type=\"submit\"\n          onClick={signIn}\n        >\n          Sign In\n        </Button>\n      </div>\n\n      <div className=\"login-container\" id=\"signup\">\n        <img\n          src=\"https://cdn0.iconfinder.com/data/icons/kameleon-free-pack-rounded/110/Chat-2-512.png\"\n          alt=\"\"\n        />\n        <input\n          required\n          placeholder=\"Username\"\n          value={username}\n          onChange={(e) => {\n            setUsername(e.target.value);\n          }}\n          type=\"text\"\n        />\n        <input\n          placeholder=\"Email\"\n          value={email}\n          onChange={(e) => {\n            setEmail(e.target.value);\n          }}\n          type=\"text\"\n        />\n        <input\n          placeholder=\"Password\"\n          value={pass}\n          onChange={(e) => {\n            setPass(e.target.value);\n          }}\n          type=\"password\"\n        />\n        <Button\n          style={{ color: \"#fff\", margin: \"10px\", background: \"#67d3fb\" }}\n          className=\"login-button\"\n          type=\"submit\"\n          onClick={signUp}\n        >\n          Sign Up\n        </Button>\n      </div>\n    </div>\n  );\n}\n\nexport default Login;\n","/home/abrar/Documents/React-Projec